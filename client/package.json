{
  "name": "client",
  "version": "2.4",
  "license": "MIT",
  "includeNodeModules": {},
  "alias": {
    "common": "../common/src",
    "crypto": "./src/alias/crypto",
    "cross-fetch": "./src/alias/cross-fetch",
    "preact/jsx-dev-runtime": "preact/jsx-runtime",
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "targets": {
    "test": {
      "distDir": "dist/test",
      "includeNodeModules": false,
      "context": "node",
      "engines": {
        "node": "16"
      }
    },
    "www": {
      "includeNodeModules": {},
      "source": "src/index.html"
    }
  },
  "old": {
    "targets": {
      "modern": {
        "includeNodeModules": {},
        "engines": {
          "browsers": "Chrome 80"
        }
      },
      "legacy": {
        "includeNodeModules": {},
        "engines": {
          "browsers": "> 0.5%, last 2 versions, not dead"
        }
      },
      "electron": {
        "includeNodeModules": {},
        "source": "src/electron/main.ts",
        "engines": {
          "node": "16"
        },
        "context": "electron-main"
      }
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "prebuild": "node prebuild.js",
    "parcel": "parcel",
    "start": "parcel",
    "build": "parcel build",
    "watch": "parcel watch",
    "electron": "parcel build --target electron && electron dist/electron/main.js",
    "clean:parcel": "rm -r .parcel-cache",
    "clean:dist": "rm -r dist",
    "clean": "yarn clean:parcel && yarn clean:dist",
    "prettify": "prettier --write \"src/**/*\"",
    "pretest": "parcel build \"./src/**/*.spec.ts\" --target test",
    "test": "jest dist/test --env jsdom --verbose",
    "lint": "eslint",
    "check": "tsc --noEmit",
    "ci": "yarn prettify && yarn build && yarn test && yarn lint && yarn check",
    "report": "npx parcel build --reporter @parcel/reporter-bundle-analyzer"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^29.2.3",
    "@types/react-dom": "^18.0.6",
    "crypto-browserify": "^3.12.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "parcel": "^2.7.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.2",
    "date-fns": "^2.29.2",
    "deep-equal": "^2.0.5",
    "electron": "^20.0.3",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.9.1",
    "normalize.css": "^8.0.1",
    "preact": "^10.11.1",
    "preact-iso": "^2.3.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "tinycolor2": "^1.4.2",
    "yup": "^0.32.11"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true
  }
}
